{
  "name": "docs",
  "version": "1.8.0",
  "private": true,
  "description": "docs of KduPress",
  "keywords": [
    "documentation",
    "generator",
    "kdu"
  ],
  "homepage": "https://github.com/kdujs/kdupress#readme",
  "bugs": {
    "url": "https://github.com/kdujs/kdupress/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kdujs/kdupress.git"
  },
  "license": "MIT",
  "author": "NKDuy",
  "scripts": {
    "build": "kdupress build docs --temp .temp",
    "dev": "kdupress dev docs --temp .temp",
    "lint-md": "yarn lint-md:style && yarn lint-md:wording",
    "lint-md:style": "remark --quiet --frail .",
    "lint-md:wording": "textlint ./docs/**/*.md",
    "show-help": "kdupress --help",
    "view-info": "kdupress view-info docs --temp .temp"
  },
  "devDependencies": {
    "@kdupress/plugin-back-to-top": "1.8.0",
    "@kdupress/plugin-google-analytics": "1.8.0",
    "@kdupress/plugin-medium-zoom": "1.8.0",
    "@kdupress/plugin-pwa": "1.8.0",
    "@kdupress/theme-kdu": "1.8.0",
    "@textlint-rule/textlint-rule-no-unmatched-pair": "^1.0.7",
    "kdupress": "1.8.0",
    "kdupress-plugin-flowchart": "^1.4.2",
    "remark-cli": "^9.0.0",
    "remark-lint": "^6.0.5",
    "remark-preset-lint-consistent": "^2.0.3",
    "remark-preset-lint-recommended": "^3.0.3",
    "textlint": "^11.3.1",
    "textlint-filter-rule-comments": "^1.2.2",
    "textlint-rule-apostrophe": "^1.0.0",
    "textlint-rule-common-misspellings": "^1.0.1",
    "textlint-rule-diacritics": "^1.0.0",
    "textlint-rule-en-capitalization": "^2.0.2",
    "textlint-rule-stop-words": "^2.0.8",
    "textlint-rule-terminology": "^2.1.4",
    "textlint-rule-write-good": "^1.6.2"
  }
}
